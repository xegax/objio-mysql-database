{
  "name": "objio-mysql-database",
  "version": "0.0.36",
  "description": "",
  "main": "index.js",
  "repository": "git@github.com:xegax/objio-mysql-database.git",
  "scripts": {
    "prepare": "npm run clean && tsc -p tsconfig.publish.json -outDir . && npm run copy",
    "postpublish": "npm run clean",
    "dist": "tsc",
    "dev": "npm run clean-build && tsc -p tsconfig.publish.json -outDir build && shx cp package.json build",
    "clean": "npm run clean-src && npm run clean-dirs",
    "clean-src": "shx rm -rf src/**/*.js src/**/*.js.map src/**/*.d.ts *.d.ts *.js *.js.map",
    "clean-build": "shx rm -rf build/**/*",
    "copy": "shx rm -rf images && shx cp -rf ./src/images ./images",
    "clean-dirs": "shx rm -rf client server task view"
  },
  "author": "xega",
  "license": "ISC",
  "devDependencies": {
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6"
  },
  "dependencies": {
    "mysql": "^2.16.0",
    "objio": "^0.0.36",
    "objio-object": "^0.0.36",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "ts-react-ui": "^0.1.36"
  }
}
